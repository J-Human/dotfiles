set nocompatible
filetype on
filetype plugin on
filetype indent on
syntax on
set number
set shiftwidth=2
set tabstop=2
set nobackup
set nowrap
set incsearch
set smartcase
set showmatch
set hlsearch
set wildmenu
set laststatus=2
set noswapfile
set spell spelllang=en_us
set ft=unix
set shell=/bin/zsh
set re=0
